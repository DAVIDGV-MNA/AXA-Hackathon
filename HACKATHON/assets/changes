$body = @{
  api_version = "2024-10-21"
  model_url   = "https://api-int.se.axa-go.applications.services.axa-tech.intraxa/ago-m365-securegpt-hub-v1-vrs/providers/openai/deployments/gpt-4o-2024-11-20/chat/completions?api-version=2024-10-21"
  messages    = @(@{ role="user"; content="hello" })
} | ConvertTo-Json

Invoke-WebRequest -Uri http://127.0.0.1:3001/securegpt/chat -Method POST `
  -ContentType 'application/json' -Body $body | Select-Object -ExpandProperty Content