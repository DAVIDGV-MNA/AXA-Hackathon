# Reindex
Invoke-RestMethod -Method Post -Uri "http://localhost:8000/rag/reindex"

# Summary (10 rows)
Invoke-RestMethod -Uri "http://localhost:8000/rag/summary"

# Summary (25 rows)
Invoke-RestMethod -Uri "http://localhost:8000/rag/summary?n=25"

# CSV export
Invoke-RestMethod -Uri "http://localhost:8000/rag/summary/csv"